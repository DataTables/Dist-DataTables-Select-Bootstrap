<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <version>2.0.1</version>
        <id>datatables.net-select-bs</id>
        <description>This is Select for DataTables with styling for [Bootstrap](https://getbootstrap.com/docs/3.3/)</description>
        <repository type="git" url="https://github.com/DataTables/Dist-DataTables-Select-Bootstrap.git"/>
        <authors>SpryMedia Ltd</authors>
        <projectUrl>http://datatables.net</projectUrl>
        <license type="expression">MIT</license>
        <tags>select  selection  Bootstrap  Datatables jQuery table</tags>
        <dependencies>
            <dependency id="datatables.net-select" version="1.4.0"/>
            <dependency id="datatables.net-bs" version="1.12.1"/>
        </dependencies>
        <contentFiles>
            <files include="**/*" buildAction="Content"/>
        </contentFiles>
    </metadata>
    <files>
        <file src="js/select.bootstrap.js" target="content\Scripts"/>
        <file src="js/select.bootstrap.js" target="contentFiles\any\any\wwwroot\js"/>
        <file src="js/select.bootstrap.min.js" target="content\Scripts"/>
        <file src="js/select.bootstrap.min.js" target="contentFiles\any\any\wwwroot\js"/>
        <file src="css/select.bootstrap.css" target="content\Content"/>
        <file src="css/select.bootstrap.css" target="contentFiles\any\any\wwwroot\css"/>
        <file src="css/select.bootstrap.min.css" target="content\Content"/>
        <file src="css/select.bootstrap.min.css" target="contentFiles\any\any\wwwroot\css"/>
    </files>
</package>

